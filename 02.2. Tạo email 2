#include <bits/stdc++.h>
using namespace std;
int main(){
    int test; cin >> test;
    cin.ignore();
    map<string, int>mp;
    while(test--){
        string s;
        getline(cin ,s);
        for(int i = 0; i < s.length(); i++) s[i] = tolower(s[i]);
        stringstream ss(s);
        string temp;
        vector<string> email;
        while(ss >> temp) email.push_back(temp);
        string x = email[email.size() - 1];
        for(int i = 0; i < email.size() - 1; i++){
            x += email[i][0];
        }
        mp[x]++;
        if(mp[x] != 1) cout << x << mp[x];
        else cout << x;
        cout << "@gmail.com" << endl;
    }
}
