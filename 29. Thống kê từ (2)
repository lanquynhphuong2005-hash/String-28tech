#include<bits/stdc++.h>
using namespace std;

bool cmp(pair<string,int> a, pair<string,int> b){
    if(a.second != b.second) return a.second > b.second;
    else return a.first < b.first;
}
int main(){
    int n; cin >> n; 
    map<string,int> mp;
    cin.ignore();
    while(n--)
   {string s;
    getline(cin,s );
     for (char &c : s) {
        if (c == '.' || c == ',' || c == '!' || c == '-') {
            c = ' ';
        }
    }
    stringstream ss(s);
     string tmp;
     while(ss>>tmp){
        mp[tmp]++;
     }
    
}
     vector<pair<string, int>> v;
     for(auto it : mp){
        v.push_back(it);
     }
     sort(v.begin(), v.end(), cmp);
     for(auto it : v){
        cout << it.first << " " << it.second << endl;
     }


}
