#include <bits/stdc++.h>
using namespace std;
int main(){
    int test; cin >> test;
    while(test--){
        string s; cin >> s;
        map<char, int>mp;
        for(char &x : s) mp[x]++;
        int ok = 0;
        for(int i = 'a'; i <= 'z'; i++){
            if(mp[i] == 0){
                cout << "NO"; 
                ok = 1;
                break;
            }
        }
        if(ok == 0) cout << "YES";
        cout << endl;
    }
}
