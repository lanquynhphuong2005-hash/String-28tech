    int main(){
    int t; cin >> t;
    cin.ignore();
    while(t--)
    {string s;
    int k;
    cin >> s;
    cin >> k;
    map<char, int> mp;
    for(char &x : s){
        mp[x]++;
    }
    int count =0;
    for(int i = 'a'; i<='z'; i++){
        if(mp[i]==0){
            count ++;
        }
    }
    if(count > k) cout << "0\n";
    else cout << "1\n";

    }

   
    }
