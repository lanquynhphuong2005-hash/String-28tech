#include <bits/stdc++.h>
using namespace std;
int main(){
    int test; cin >> test;
    while(test--){
        string s; cin >> s;
        int count = 0;
        for(int i = 0; i < s.length(); i++){
            for(int j = i; j < s.length(); j++){
                if(s[i] == s[j]){
                    count++;
                }
            }
        }
        cout << count << endl;
    }
}
